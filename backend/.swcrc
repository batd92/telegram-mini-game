{
    "$schema": "https://json.schemastore.org/swcrc",
    "sourceMaps": true,
    "module": {
        "type": "commonjs"
    },
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "decorators": true,
            "dynamicImport": true
        },
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
        },
        "baseUrl": ".",
        "paths": {
            "@filters/*": ["./src/filters/*"],
            "@interceptors/*": ["./src/interceptors/*"],
            "@config/*": ["./src/config/*"],
            "@constants/*": ["./src/constants/*"],
            "*": [
                "node_modules",
                "src/*"
            ]
        }
    },
    "minify": false
}